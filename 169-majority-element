class Solution:
    def majorityElement(self, nums: List[int]) -> int:
        """
        dict = {}
        for i in nums:
            
            if i in dict:
                dict[i] += 1
            else:
                dict[i] = 1
                
            if dict[i] > len(nums)//2:
                return i
        """
        
        nums.sort()
        return nums[len(nums)//2]
            
